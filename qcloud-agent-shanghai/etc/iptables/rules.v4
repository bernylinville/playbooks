*filter
:INPUT ACCEPT [0:0]
:FORWARD ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
COMMIT

*nat
:PREROUTING ACCEPT [0:0]
:INPUT ACCEPT [0:0]
:OUTPUT ACCEPT [0:0]
:POSTROUTING ACCEPT [0:0]

# linode-agent-sg
-A PREROUTING -p tcp -m tcp --dport 443 -j DNAT --to-destination 139.162.57.130:443
-A POSTROUTING -d 139.162.57.130/32 -p tcp -m tcp --dport 443 -j SNAT --to-source 10.105.197.50

# digitalocean-agent-sg
-A PREROUTING -p tcp -m tcp --dport 1443 -j DNAT --to-destination 128.199.140.124:443
-A POSTROUTING -d 128.199.140.124/32 -p tcp -m tcp --dport 443 -j SNAT --to-source 10.105.197.50

# digitalocean-agent-us3
-A PREROUTING -p tcp -m tcp --dport 2443 -j DNAT --to-destination 104.236.197.143:443
-A POSTROUTING -d 104.236.197.143/32 -p tcp -m tcp --dport 443 -j SNAT --to-source 10.105.197.50

COMMIT
